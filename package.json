{
  "name": "vue-froalacharts",
  "version": "1.0.1",
  "description": "Simple and lightweight official Vue component for FroalaCharts.",
  "main": "dist/vue-froalacharts.js",
  "author": "froala",
  "homepage": "https://github.com/froala/vue-froalacharts",
  "repository": {
    "type": "git",
    "url": "https://github.com/froala/vue-froalacharts.git"
  },
  "bugs": {
    "url": "https://github.com/froala/vue-froalacharts/issues"
  },
  "keywords": [
    "Vue",
    "Vue.js",
    "vue-froalacharts-component",
    "vue-froalacharts",
    "froalacharts",
    "javascript-charts",
    "interactive-charts",
    "charts",
    "graphs",
    "visualization",
    "data-visualization",
    "dataviz",
    "vue-charts"
  ],
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint ./src/**/*.js webpack.config.babel.js",
    "build": "webpack --config webpack.config.babel.js",
    "rollup": "rollup src/index.js -f umd -n VueFroalaCharts -g froalacharts:FroalaCharts -o dist/vue-froalacharts.js",
    "watch": "webpack --config webpack.config.babel.js -w",
    "dev": "webpack-dev-server --config example/webpack.config.js",
    "start": "webpack-dev-server --config example/webpack.config.js"
  },
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^2.0.0",
    "froalacharts": "^1.0.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "vue": "^2.3.3",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "yards": "^0.1.4"
  },
  "dependencies": {
    "underscore": "^1.9.1"
  }
